.qr_enter {
  margin: 64px auto 0;
}

.qr_enter .key {
  text-align: center;
}

.qr_enter .ec {
  text-align: center;
  margin: 5px 0 0;
  color: #fff;
}

.qr_enter .inp {
  width: 100%;
  height: 100%;
  text-align: center;
  justify-content: center;
  margin: 54px 0 0;
  display: flex;
}

.qr_enter .inp input {
  width: 40px;
  height: 48px;
  font-size: 24px;
  text-align: center;
  background: rgba(255, 255, 255, 0.9);
  border-radius: 5px;
  margin: 0 3px;
}

.qr_enter .btn_c {
  width: 280px;
  margin: 64px auto 0;
}

.qr_enter .btn_c navigator {
  display: block;
  background: #2ed04a;
  color: #fff;
  text-align: center;
  width: 280px;
  height: 48px;
  line-height: 48px;
  font-size: 16px;
  border-radius: 100px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}

.qr_enter navigator.change {
  color: rgba(255, 255, 255, 0.75);
  text-align: center;
  display: block;
  margin: 16px 0 0;
}
