.coll_ps {
  position: absolute;
  margin: auto;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  height: 250px;
}

.coll_ps .ps {
  width: 100%;
  text-align: center;
  font-size: 21px;
  color: #f29a38;
}

.coll_ps .in_tips {
  width: 100%;
  font-size: 14px;
  color: 635249;
  text-align: center;
  margin: 30px 0 0;
}

.coll_ps .btn_id {
  margin: 50px 50px 0;
}

.coll_ps .btn_id navigator.i,.coll_ps .btn_id button.i {
  width: 100%;
  display: block;
  text-align: center;
  border-radius: 5px;
  font-size: 16px;
  color: #fff;
  height: 36px;
  line-height: 36px;
  background: -webkit-linear-gradient(180deg, #f0bf5f, #f79f66);
  background: -o-linear-gradient(180deg, #f0bf5f, #f79f66);
  background: -moz-linear-gradient(180deg, #f0bf5f, #f79f66);
  background: linear-gradient(180deg, #f0bf5f, #f79f66);
}

.coll_ps .btn_id navigator.d,.coll_ps .btn_id button.d {
  margin: 25px 0 0;
  width: 100%;
  display: block;
  text-align: center;
  border-radius: 5px;
  font-size: 16px;
  color: #fff;
  height: 36px;
  line-height: 36px;
  background: -webkit-linear-gradient(180deg, #89dbd5, #06ceae);
  background: -o-linear-gradient(180deg, #89dbd5, #06ceae);
  background: -moz-linear-gradient(180deg, #89dbd5, #06ceae);
  background: linear-gradient(180deg, #89dbd5, #06ceae);
}
