.title navigator.edit {
  position: absolute;
  right: 15px;
  color: #fff;
  top: 48px;
  font-size: 14px;
}

.title navigator.edit .i {
  width: 15px;
  height: 15px;
  margin: 3px 3px 2px 0;
  background: url(https://wxapp.seeyoo-tech.cn/bd/wx/icon_edit.png) no-repeat;
  display: block;
  float: left;
}

.sc_box {
  width: 100%;
  height: 80px;
  display: block;
  padding: 5px 0 0;
  /* background: -webkit-linear-gradient(90deg, #1e69c8, #0d3897);
  background: -o-linear-gradient(90deg, #1e69c8, #0d3897);
  background: -moz-linear-gradient(90deg, #1e69c8, #0d3897);
  background: linear-gradient(90deg, #1e69c8, #0d3897); */
  background: #1E69C8;
}

.sc_box .box {
  width: calc(100% - 24px);
  margin: 0 auto 14px;
  background: #fff;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.05);
  border-radius: 10px;
  position: relative;
}

.sc_box .box .h_shop {
  width: 100%;
  height: 42px;
  border-bottom: 0.5px solid #f2f1f0;
}

.sc_box .box .h_shop navigator {
  display: block;
  width: 108px;
  color: #383b41;
  line-height: 42px;
  margin: 0 15px;
  font-size: 12px;
}

.sc_box .box .h_shop navigator .i_round {
  width: 18px;
  height: 18px;
  margin: 12px 12px 12px 0;
  border-radius: 100%;
  border: 1px solid #82858b;
  display: block;
  float: left;
}

.sc_box .box .h_shop navigator .i_shop {
  float: left;
  width: 12px;
  height: 12px;
  margin: 15px 4px 15px 0;
  background: url(https://wxapp.seeyoo-tech.cn/bd/wx/icon_shop.png) no-repeat;
}

.sc_box .box .p_blo {
  width: 100%;
}

.sc_box .box .p_blo .li {
  border-bottom: 0.5px solid #f2f1f0;
  padding: 12px 15px;
  position: relative;
}

.sc_box .box .p_blo .li navigator.round {
  display: block;
  float: left;
  margin: 36px 0;
}

.sc_box .box .p_blo .li navigator.round .i_round {
  width: 18px;
  height: 18px;
  border-radius: 100%;
  border: 1px solid #82858b;
  display: block;
  float: left;
}

.sc_box .box .p_blo .li navigator:hover .i_round {
  width: 18px;
  height: 18px;
  border-radius: 100%;
  border: 1px solid #0d3897;
  background: #0d3897 url(https://wxapp.seeyoo-tech.cn/bd/wx/icon_hook.png) center center no-repeat;
}

.sc_box .box .p_blo .li .img {
  width: 120rpx;
  height: 168rpx;
  float: left;
  text-align: center;
}

.sc_box .box .p_blo .li .img image {
  height: 72px;
  width: 100%;
  margin: 6px 0;
}

.sc_box .box .p_blo .li .c_txt {
  float: left;
}

.sc_box .box .p_blo .li .c_txt .h4 {
  font-size: 13px;
  margin: 0 0 4px;
}

.sc_box .box .p_blo .li .c_txt text {
  font-size: 12px;
  color: #82858b;
  display: block;
}

.sc_box .box .p_blo .li .pri {
  position: absolute;
  right: 15px;
  bottom: 12px;
  color: #fe000d;
}

.fs {
  width: 100%;
  margin: auto;
  position: fixed;
  bottom: 0;
  z-index: 99;
  background: #fff;
  height: 48px;
  border-top: 0.5px solid #82858b;
}

.fs .total {
  float: left;
  margin: 14px 12px;
}

.fs .total text {
  color: #fe000d;
}

.fs .btn {
  float: right;
  margin: 6px 12px;
}

.fs .btn navigator.l_b {
  width: 90px;
  font-size: 14px;
  float: left;
  text-align: center;
  height: 36px;
  line-height: 36px;
  color: #fff;
  background: #0d3897;
  border-radius: 100px;
  border: 1px solid #0d3897;
  display: block;
}
